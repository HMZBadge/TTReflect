platform :ios, '8.0'

target 'TTReflect' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire', '~> 3.0'
  pod 'AFNetworking', '2.6.3'
  pod 'SwiftyJSON', '~> 2.3.2'
  # Pods for TTReflect

  target 'TTReflectTests' do
    inherit! :search_paths
    pod 'Alamofire', '~> 3.0'
    pod 'AFNetworking', '2.6.3'
    pod 'SwiftyJSON', '~> 2.3.2'
    # Pods for testing
  end

  target 'TTReflectUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
